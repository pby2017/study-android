<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/frame_for_drag"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/text_first" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/text_second" />

    <com.example.customcircleapplication.custom.CustomStrokedCircleView
        android:id="@+id/custom_stroked_circle"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:setRadius="300"
        app:setStrokeWidth="10" />
</FrameLayout>

